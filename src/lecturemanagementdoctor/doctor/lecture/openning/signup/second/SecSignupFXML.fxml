            <font>
                <Font size="15.0" />
            </font>
        </JFXTextField>
        <JFXTextField fx:id="Academic_ID" focusColor="#2ecc71" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" prefWidth="267.0" promptText="Academic ID" style="-fx-text-fill: #2ecc71;" unFocusColor="#6ae88e">
            <font>
                <Font size="15.0" />
            </font>
        </JFXTextField>
      <JFXTextField fx:id="department" focusColor="#2ecc71" labelFloat="true" maxWidth="1.7976931348623157E308" prefHeight="45.0" prefWidth="267.0" promptText="Department" style="-fx-text-fill: #2ecc71;" unFocusColor="#6ae88e">
         <font>
            <Font size="15.0" />
